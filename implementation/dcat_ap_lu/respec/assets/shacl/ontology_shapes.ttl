@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix core-res: <https://mindig_lu.gitlab.io/DCAT-AP-LU#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

core-res:dcat-DatasetShape a sh:NodeShape ;
    rdfs:label "Dataset" ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:dcat-Dataset-adms-sample,
        core-res:dcat-Dataset-dct-title ;
    sh:targetClass dcat:Dataset .

core-res:dcat-DistributionShape a sh:NodeShape ;
    rdfs:label "Distribution" ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass dcat:Distribution .

core-res:xsd-stringShape a sh:NodeShape ;
    rdfs:label "String" ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass xsd:string .

core-res:dcat-Dataset-adms-sample a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcat:Distribution ;
    sh:name "Sample" ;
    sh:path adms:sample ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/adms#sample> ?that . ?that <http://www.w3.org/ns/adms#sample> ?this .}" ] .

core-res:dcat-Dataset-dct-title a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:datatype xsd:string ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Title" ;
    sh:path dcterms:title .

core-res:core-shape a owl:Ontology ;
    rdfs:label """This artefact provides the generic datashape specifications for the eProcurement Ontology Core.
        The DCAT-AP-LU describes objects and concepts, with definitions, attributes and relationships which are used for sharing information about Catalogues containing Datasets and Data Services descriptions in Europe.
        The provision of these concepts provides the basis for a common understanding of the domain for all stakeholders ensuring the quality of data exchange and transparency."""@en ;
    dcterms:created "2025-12-15"^^xsd:date ;
    dcterms:description """This artefact provides the generic datashape specifications for the DCAT-AP-LU.
        The DCAT-AP-LU describes objects and concepts, with definitions, attributes and relationships which are used for sharing information about Catalogues containing Datasets and Data Services descriptions in Europe.
        The provision of these concepts provides the basis for a common understanding of the domain for all stakeholders ensuring the quality of data exchange and transparency."""@en ;
    dcterms:issued "2026-01-12"^^xsd:date ;
    dcterms:license "Copyright 2025 Minist√®re de la Digitalisation, Luxembourg. All material in this repository is published under the license CC-BY 4.0, unless explicitly otherwise mentioned." ;
    dcterms:publisher "https://mindig_lu.gitlab.io/DCAT-AP-LU" ;
    dcterms:title "DCAT-AP-LU shapes"@en ;
    vann:preferredNamespacePrefix "dal" ;
    vann:preferredNamespaceUri "https://mindig_lu.gitlab.io/DCAT-AP-LU/#" ;
    rdfs:comment "This version is automatically generated from dcat_ap_lu_CM.xml on 2026-01-12" ;
    rdfs:seeAlso <https://mindig_lu.gitlab.io/DCAT-AP-LU/> ;
    owl:imports dcterms:,
        skos:,
        <http://www.w3.org/ns/adms>,
        <https://data.europa.eu/m8g>,
        core-res:core,
        core-res:core-restriction ;
    owl:incompatibleWith "" ;
    owl:priorVersion "https://mindig_lu.gitlab.io/DCAT-AP-LU#core-shape-1.0.0" ;
    owl:versionIRI core-res:core-shape-1.1.0 ;
    owl:versionInfo "1.1.0" .

