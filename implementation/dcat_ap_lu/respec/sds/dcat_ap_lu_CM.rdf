    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
    <skos:prefLabel xml:lang="en">date</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2001/XMLSchema#dateTime">
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Datatype"/>
    <skos:prefLabel xml:lang="en">date Time</skos:prefLabel>
    <skos:definition xml:lang="en">Object with integer-valued year, month, day, hour and minute properties, a decimal-valued second property, and a boolean timezoned property.</skos:definition>
  </rdf:Description>
