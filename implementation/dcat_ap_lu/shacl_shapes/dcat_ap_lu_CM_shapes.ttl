@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix core-res: <https://mindig_lu.gitlab.io/DCAT-AP-LU#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcatap: <http://data.europa.eu/r5r/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dpv: <https://w3id.org/dpv#> .
@prefix eli: <http://data.europa.eu/eli/ontology#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix locn: <http://www.w3.org/ns/locn#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix spdx: <http://spdx.org/rdf/terms#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

core-res:adms-IdentifierShape a sh:NodeShape ;
    rdfs:label "Identifier" ;
    rdfs:comment "This is based on the UN/CEFACT Identifier class." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass adms:Identifier .

core-res:dcat-CatalogRecordShape a sh:NodeShape ;
    rdfs:label "Catalog record" ;
    rdfs:comment "A description of a Catalogued Resource's entry in the Catalogue." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:dcat-CatalogRecord-foaf-primaryTopic ;
    sh:targetClass dcat:CatalogRecord .

core-res:dcat-CatalogShape a sh:NodeShape ;
    rdfs:label "Catalog" ;
    rdfs:comment "A catalogue or repository that hosts the Datasets or Data Services being described." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:dcat-Catalog-dcat-record,
        core-res:dcat-Catalog-dcat-service,
        core-res:dcat-Catalog-dct-creator,
        core-res:dcat-Catalog-dct-license,
        core-res:dcat-Catalog-dct-publisher ;
    sh:targetClass dcat:Catalog .

core-res:dcat-DataServiceShape a sh:NodeShape ;
    rdfs:label "Data service" ;
    rdfs:comment "A collection of operations that provides access to one or more datasets or data processing functions." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:dcat-DataService-dcat-servesDataset ;
    sh:targetClass dcat:DataService .

core-res:dcat-DatasetSeriesShape a sh:NodeShape ;
    rdfs:label "Dataset series" ;
    rdfs:comment "A collection of datasets that are published separately, but share some characteristics that group them." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:dcat-DatasetSeries-dcat-contactPoint,
        core-res:dcat-DatasetSeries-dcatap-applicableLegislation,
        core-res:dcat-DatasetSeries-dct-publisher,
        core-res:dcat-DatasetSeries-dct-spatial ;
    sh:targetClass dcat:DatasetSeries .

core-res:dcat-DatasetShape a sh:NodeShape ;
    rdfs:label "Dataset" ;
    rdfs:comment "A conceptual entity that represents the information published." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:dcat-Dataset-adms-identifier,
        core-res:dcat-Dataset-adms-sample,
        core-res:dcat-Dataset-adms-versionNotes,
        core-res:dcat-Dataset-dcat-contactPoint,
        core-res:dcat-Dataset-dcat-distribution,
        core-res:dcat-Dataset-dcat-hasVersion,
        core-res:dcat-Dataset-dcat-inSeries,
        core-res:dcat-Dataset-dcat-keyword,
        core-res:dcat-Dataset-dcat-landingPage,
        core-res:dcat-Dataset-dcat-qualifiedRelation,
        core-res:dcat-Dataset-dcat-spatialResolutionInMeters,
        core-res:dcat-Dataset-dcat-temporalResolution,
        core-res:dcat-Dataset-dcat-theme,
        core-res:dcat-Dataset-dcat-version,
        core-res:dcat-Dataset-dcatap-applicableLegislation,
        core-res:dcat-Dataset-dct-accrualPeriodicity,
        core-res:dcat-Dataset-dct-conformsTo,
        core-res:dcat-Dataset-dct-creator,
        core-res:dcat-Dataset-dct-description,
        core-res:dcat-Dataset-dct-identifier,
        core-res:dcat-Dataset-dct-isReferencedBy,
        core-res:dcat-Dataset-dct-issued,
        core-res:dcat-Dataset-dct-language,
        core-res:dcat-Dataset-dct-modified,
        core-res:dcat-Dataset-dct-provenance,
        core-res:dcat-Dataset-dct-publisher,
        core-res:dcat-Dataset-dct-relation,
        core-res:dcat-Dataset-dct-rights,
        core-res:dcat-Dataset-dct-source,
        core-res:dcat-Dataset-dct-spatial,
        core-res:dcat-Dataset-dct-temporal,
        core-res:dcat-Dataset-dct-title,
        core-res:dcat-Dataset-dct-type,
        core-res:dcat-Dataset-dpv-hasData,
        core-res:dcat-Dataset-foaf-page,
        core-res:dcat-Dataset-prov-qualifiedAttribution,
        core-res:dcat-Dataset-prov-wasGeneratedBy ;
    sh:targetClass dcat:Dataset .

core-res:dcat-DistributionShape a sh:NodeShape ;
    rdfs:label "Distribution" ;
    rdfs:comment "A physical embodiment of the Dataset in a particular format." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:dcat-Distribution-adms-status,
        core-res:dcat-Distribution-dcat-accessService,
        core-res:dcat-Distribution-dcat-accessURL,
        core-res:dcat-Distribution-dcat-byteSize,
        core-res:dcat-Distribution-dcat-compressFormat,
        core-res:dcat-Distribution-dcat-downloadURL,
        core-res:dcat-Distribution-dcat-mediaType,
        core-res:dcat-Distribution-dcat-packageFormat,
        core-res:dcat-Distribution-dcat-spatialResolutionInMeters,
        core-res:dcat-Distribution-dcat-temporalResolution,
        core-res:dcat-Distribution-dcatap-applicableLegislation,
        core-res:dcat-Distribution-dcatap-availability,
        core-res:dcat-Distribution-dct-conformsTo,
        core-res:dcat-Distribution-dct-description,
        core-res:dcat-Distribution-dct-format,
        core-res:dcat-Distribution-dct-issued,
        core-res:dcat-Distribution-dct-language,
        core-res:dcat-Distribution-dct-license,
        core-res:dcat-Distribution-dct-modified,
        core-res:dcat-Distribution-dct-rights,
        core-res:dcat-Distribution-dct-title,
        core-res:dcat-Distribution-foaf-page,
        core-res:dcat-Distribution-odrl-hasPolicy,
        core-res:dcat-Distribution-spdx-checksum ;
    sh:targetClass dcat:Distribution .

core-res:dcat-RelationshipShape a sh:NodeShape ;
    rdfs:label "Relationship" ;
    rdfs:comment "An association class for attaching additional information to a relationship between DCAT Resources." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass dcat:Relationship .

core-res:dcat-ResourceShape a sh:NodeShape ;
    rdfs:label "Resource" ;
    rdfs:comment "Resource published or curated by a single agent." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass dcat:Resource .

core-res:dct-FrequencyShape a sh:NodeShape ;
    rdfs:label "Frequency" ;
    rdfs:comment "A rate at which something recurs, e.g. the publication of a Dataset." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass dcterms:Frequency .

core-res:dct-LicenseDocumentShape a sh:NodeShape ;
    rdfs:label "License document" ;
    rdfs:comment "A legal document giving official permission to do something with a resource." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:dct-LicenseDocument-dct-type ;
    sh:targetClass dcterms:LicenseDocument .

core-res:dct-LinguisticSystemShape a sh:NodeShape ;
    rdfs:label "Linguistic system" ;
    rdfs:comment "A system of signs, symbols, sounds, gestures, or rules used in communication, e.g. a language." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass dcterms:LinguisticSystem .

core-res:dct-LocationShape a sh:NodeShape ;
    rdfs:label "Location" ;
    rdfs:comment "A temporal period that the Dataset covers." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:dct-Location-locn-geometry ;
    sh:targetClass dcterms:Location .

core-res:dct-MediaTypeOrExtentShape a sh:NodeShape ;
    rdfs:label "Media type or extent" ;
    rdfs:comment "A media type or dataset distribution." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass dcterms:MediaTypeOrExtent .

core-res:dct-MediaTypeShape a sh:NodeShape ;
    rdfs:label "Media type" ;
    rdfs:comment "A file format or physical medium." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass dcterms:MediaType .

core-res:dct-PeriodOfTimeShape a sh:NodeShape ;
    rdfs:label "Period of time" ;
    rdfs:comment "An interval of time that is named or defined by its start and end dates." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:dct-PeriodOfTime-dcat-endDate,
        core-res:dct-PeriodOfTime-dcat-startDate,
        core-res:dct-PeriodOfTime-time-hasBeginning,
        core-res:dct-PeriodOfTime-time-hasEnd ;
    sh:targetClass dcterms:PeriodOfTime .

core-res:dct-ProvenanceStatementShape a sh:NodeShape ;
    rdfs:label "Provenance statement" ;
    rdfs:comment "A statement of any changes in ownership and custody of a resource since its creation that are significant for its authenticity, integrity, and interpretation." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass dcterms:ProvenanceStatement .

core-res:dct-RightsStatementShape a sh:NodeShape ;
    rdfs:label "Rights statement" ;
    rdfs:comment "A statement about the intellectual property rights (IPR) held in or over a resource, a legal document giving official permission to do something with a resource, or a statement about access rights." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass dcterms:RightsStatement .

core-res:dct-StandardShape a sh:NodeShape ;
    rdfs:label "Standard" ;
    rdfs:comment "A standard or other specification to which a Dataset or Distribution conforms." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass dcterms:Standard .

core-res:dpv-DataShape a sh:NodeShape ;
    rdfs:label "Data" ;
    rdfs:comment "A broad concept representing 'data' or 'information'." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass dpv:Data .

core-res:eli-LegalResourceShape a sh:NodeShape ;
    rdfs:label "Legal resource" ;
    rdfs:comment "This class represents the legislation, policy or policies that lie behind the Rules that govern the service." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass eli:LegalResource .

core-res:foaf-AgentShape a sh:NodeShape ;
    rdfs:label "Agent" ;
    rdfs:comment "Any entity carrying out actions with respect to the entities Catalogue and the Catalogued Resources." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:foaf-Agent-dct-type ;
    sh:targetClass foaf:Agent .

core-res:foaf-DocumentShape a sh:NodeShape ;
    rdfs:label "Document" ;
    rdfs:comment "A textual resource intended for human consumption that contains information, e.g. a web page about a Dataset." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass foaf:Document .

core-res:locn-GeometryShape a sh:NodeShape ;
    rdfs:label "Geometry" ;
    rdfs:comment "The Geometry class provides the means to identify a Location as a point, line, polygon, etc. expressed using coordinates in some coordinate reference System." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass locn:Geometry .

core-res:odrl-PolicyShape a sh:NodeShape ;
    rdfs:label "Policy" ;
    rdfs:comment "A non-empty group of Permissions and/or Prohibitions." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass odrl:Policy .

core-res:prov-ActivityShape a sh:NodeShape ;
    rdfs:label "Activity" ;
    rdfs:comment "An activity is something that occurs over a period of time and acts upon or with entities; it may include consuming, processing, transforming, modifying, relocating, using, or generating entities." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass prov:Activity .

core-res:prov-AttributionShape a sh:NodeShape ;
    rdfs:label "Attribution" ;
    rdfs:comment "Attribution is the ascribing of an entity to an agent." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass prov:Attribution .

core-res:skos-ConceptShape a sh:NodeShape ;
    rdfs:label "Concept" ;
    rdfs:comment "An idea or notion; a unit of thought." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:property core-res:skos-Concept-skos-prefLabel ;
    sh:targetClass skos:Concept .

core-res:spdx-ChecksumShape a sh:NodeShape ;
    rdfs:label "Checksum" ;
    rdfs:comment "A Checksum is value that allows the contents of a file to be authenticated. Even small changes to the content of the file will change its checksum. This class allows the results of a variety of checksum and cryptographic message digest algorithms to be represented." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass spdx:Checksum .

core-res:vcard-KindShape a sh:NodeShape ;
    rdfs:label "Kind" ;
    rdfs:comment "A description following the vCard specification, e.g. to provide telephone number and e-mail address for a contact point." ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:targetClass vcard:Kind .

core-res:dcat-Catalog-dcat-record a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcat:CatalogRecord ;
    sh:description "A Catalogue Record that is part of the Catalogue." ;
    sh:name "Record" ;
    sh:path dcat:record ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#record> ?that . ?that <http://www.w3.org/ns/dcat#record> ?this .}" ] .

core-res:dcat-Catalog-dcat-service a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcat:DataService ;
    sh:description "A site or end-point (Data Service) that is listed in the Catalogue." ;
    sh:name "Service" ;
    sh:path dcat:service ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#service> ?that . ?that <http://www.w3.org/ns/dcat#service> ?this .}" ] .

core-res:dcat-Catalog-dct-creator a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class foaf:Agent ;
    sh:description "An entity responsible for the creation of the catalogue." ;
    sh:maxCount 1 ;
    sh:name "Creator" ;
    sh:path dcterms:creator ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/creator> ?that . ?that <http://purl.org/dc/terms/creator> ?this .}" ] .

core-res:dcat-Catalog-dct-license a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcterms:LicenseDocument ;
    sh:description "A licence under which the Catalogue can be used or reused." ;
    sh:maxCount 1 ;
    sh:name "License" ;
    sh:path dcterms:license ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/license> ?that . ?that <http://purl.org/dc/terms/license> ?this .}" ] .

core-res:dcat-Catalog-dct-publisher a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class foaf:Agent ;
    sh:description "An entity (organisation) responsible for making the Catalogue available." ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Publisher" ;
    sh:path dcterms:publisher ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://purl.org/dc/terms/publisher> ?that . ?that <http://purl.org/dc/terms/publisher> ?this .}" ] .

core-res:dcat-CatalogRecord-foaf-primaryTopic a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcat:Resource ;
    sh:description "A link to the Dataset, Data service or Catalog described in the record." ;
    sh:maxCount 1 ;
    sh:minCount 1 ;
    sh:name "Primary topic" ;
    sh:path foaf:primaryTopic ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://xmlns.com/foaf/0.1/primaryTopic> ?that . ?that <http://xmlns.com/foaf/0.1/primaryTopic> ?this .}" ] .

core-res:dcat-DataService-dcat-servesDataset a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcat:Dataset ;
    sh:description "This property refers to a collection of data that this data service can distribute." ;
    sh:name "Serves dataset" ;
    sh:path dcat:servesDataset ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#servesDataset> ?that . ?that <http://www.w3.org/ns/dcat#servesDataset> ?this .}" ] .

core-res:dcat-Dataset-adms-identifier a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class adms:Identifier ;
    sh:description "A secondary identifier of the Dataset." ;
    sh:name "Identifier" ;
    sh:path adms:identifier ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/adms#identifier> ?that . ?that <http://www.w3.org/ns/adms#identifier> ?this .}" ] .

core-res:dcat-Dataset-adms-sample a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcat:Distribution ;
    sh:description "A sample distribution of the dataset." ;
    sh:name "Sample" ;
    sh:path adms:sample ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/adms#sample> ?that . ?that <http://www.w3.org/ns/adms#sample> ?this .}" ] .

core-res:dcat-Dataset-adms-versionNotes a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:datatype rdfs:Literal ;
    sh:description "A description of the differences between this version and a previous version of the Dataset." ;
    sh:name "Version notes" ;
    sh:path adms:versionNotes .

core-res:dcat-Dataset-dcat-contactPoint a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class vcard:Kind ;
    sh:description "Contact information that can be used for sending comments about the Dataset." ;
    sh:name "Contact point" ;
    sh:path dcat:contactPoint ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#contactPoint> ?that . ?that <http://www.w3.org/ns/dcat#contactPoint> ?this .}" ] .

core-res:dcat-Dataset-dcat-distribution a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcat:Distribution ;
    sh:description "An available Distribution for the Dataset." ;
    sh:minCount 1 ;
    sh:name "Distribution" ;
    sh:path dcat:distribution ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#distribution> ?that . ?that <http://www.w3.org/ns/dcat#distribution> ?this .}" ] .

core-res:dcat-Dataset-dcat-hasVersion a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcat:Dataset ;
    sh:description "A related Dataset that is a version, edition, or adaptation of the described Dataset." ;
    sh:name "Has version" ;
    sh:path dcat:hasVersion ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#hasVersion> ?that . ?that <http://www.w3.org/ns/dcat#hasVersion> ?this .}" ] .

core-res:dcat-Dataset-dcat-inSeries a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcat:DatasetSeries ;
    sh:description "A dataset series of which the dataset is part." ;
    sh:name "In series" ;
    sh:path dcat:inSeries ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#inSeries> ?that . ?that <http://www.w3.org/ns/dcat#inSeries> ?this .}" ] .

core-res:dcat-Dataset-dcat-keyword a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:datatype rdfs:Literal ;
    sh:description "A keyword or tag describing the Dataset." ;
    sh:name "Keyword" ;
    sh:path dcat:keyword .

core-res:dcat-Dataset-dcat-landingPage a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class foaf:Document ;
    sh:description "A web page that provides access to the Dataset, its Distributions and/or additional information." ;
    sh:name "Landing page" ;
    sh:path dcat:landingPage ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#landingPage> ?that . ?that <http://www.w3.org/ns/dcat#landingPage> ?this .}" ] .

core-res:dcat-Dataset-dcat-qualifiedRelation a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcat:Relationship ;
    sh:description "A description of a relationship with another resource." ;
    sh:name "Qualified relation" ;
    sh:path dcat:qualifiedRelation ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#qualifiedRelation> ?that . ?that <http://www.w3.org/ns/dcat#qualifiedRelation> ?this .}" ] .

core-res:dcat-Dataset-dcat-spatialResolutionInMeters a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:datatype xsd:decimal ;
    sh:description "The minimum spatial separation resolvable in a dataset, measured in metres." ;
    sh:name "Spatial resolution in meters" ;
    sh:path dcat:spatialResolutionInMeters .

core-res:dcat-Dataset-dcat-temporalResolution a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:datatype xsd:duration ;
    sh:description "The minimum time period resolvable in the dataset." ;
    sh:maxCount 1 ;
    sh:name "Temporal resolution" ;
    sh:path dcat:temporalResolution .

core-res:dcat-Dataset-dcat-theme a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class skos:Concept ;
    sh:description "A category of the Dataset." ;
    sh:name "Theme" ;
    sh:path dcat:theme ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://www.w3.org/ns/dcat#theme> ?that . ?that <http://www.w3.org/ns/dcat#theme> ?this .}" ] .

core-res:dcat-Dataset-dcat-version a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:datatype rdfs:Literal ;
    sh:description "The version indicator (name or identifier) of a resource." ;
    sh:maxCount 1 ;
    sh:name "Version" ;
    sh:path dcat:version .

core-res:dcat-Dataset-dcatap-applicableLegislation a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class eli:LegalResource ;
    sh:description "The legislation that mandates the creation or management of the Dataset." ;
    sh:name "Applicable legislation" ;
    sh:path dcatap:applicableLegislation ;
    sh:sparql [ sh:select "SELECT ?this ?that WHERE { ?this <http://data.europa.eu/r5r/applicableLegislation> ?that . ?that <http://data.europa.eu/r5r/applicableLegislation> ?this .}" ] .

core-res:dcat-Dataset-dct-accrualPeriodicity a sh:PropertyShape ;
    rdfs:isDefinedBy core-res:core-shape ;
    sh:class dcterms:Frequency ;
    sh:description "The frequency at which the Dataset is updated." ;
    sh:maxCount 1 ;
    sh:name "Accrual periodicity" ;
    sh:path dcterms:accrualPeriodicity ;
