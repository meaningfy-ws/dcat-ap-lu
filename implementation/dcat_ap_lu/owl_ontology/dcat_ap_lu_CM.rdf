  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/adms#status">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">status</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/temporal">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">temporal</skos:prefLabel>
    <skos:definition xml:lang="en">A temporal period that the Dataset covers.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/prov#wasGeneratedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">was Generated By</skos:prefLabel>
    <skos:definition xml:lang="en">provides the business context for, the creation of the dataset.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/locn#Geometry">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Geometry</skos:prefLabel>
    <skos:definition xml:lang="en">The Geometry class provides the means to identify a Location as a point, line, polygon, etc. expressed using coordinates in some coordinate reference System.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#DataService">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Data Service</skos:prefLabel>
    <skos:definition xml:lang="en">A collection of operations that provides access to one or more datasets or data processing functions.</skos:definition>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/rights">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">rights</skos:prefLabel>
    <skos:definition xml:lang="en">Information that indicates whether the Dataset is publicly accessible, has access restrictions, or is not public. A statement that specifies rights associated with the Distribution.</skos:definition>
    <skos:note xml:lang="en">DCAT-AP-LU places additional restrictions on the vocabularies recommended in DCAT-AP. See section 10. Controlled Vocabularies for details.</skos:note>
    <reuse xml:lang="en">E</reuse>
    <reuse xml:lang="en">A</reuse>
    <dcterms:references xml:lang="en">&lt; a href="https://www.w3.org/TR/vocab-dcat-3/#Property:resource_access_rights"&gt;Link&lt;/a&gt;</dcterms:references>
    <dcterms:references xml:lang="en">&lt;a href="https://www.w3.org/TR/vocab-dcat-3/#Property:distribution_rights"&gt;Link&lt;/a&gt;
</dcterms:references>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/type">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">type</skos:prefLabel>
    <skos:definition xml:lang="en">A type of the Dataset. A type of licence, e.g. indicating 'public domain' or 'royalties required'.</skos:definition>
    <reuse xml:lang="en">E</reuse>
    <skos:note xml:lang="en">DCAT-AP-LU extends the recommendations for controlled vocabularies for this property provided by DCAT-AP.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/isReferencedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <skos:prefLabel xml:lang="en">is Referenced By</skos:prefLabel>
    <skos:definition xml:lang="en">A related resource, such as a publication, that references, cites, or otherwise points to the dataset. (dcat:Dataset)</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/adms#Identifier">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Identifier</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#record">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">record</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/language">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">language</skos:prefLabel>
    <skos:definition xml:lang="en">A language of the Dataset. A language used in the Distribution.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#byteSize">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <skos:prefLabel xml:lang="en">byte Size</skos:prefLabel>
    <skos:definition xml:lang="en">The size of a Distribution in bytes. (dcat:Distribution)</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#accessURL">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <skos:prefLabel xml:lang="en">access URL</skos:prefLabel>
    <skos:definition xml:lang="en">A URL that gives access to a Distribution of the Dataset. (dcat:Distribution)</skos:definition>
    <skos:note xml:lang="en">The resource at the access URL may contain information about how to get the Dataset. </skos:note>
    <dcterms:references xml:lang="en">&lt;a href="https://www.w3.org/TR/vocab-dcat-3/#Property:distribution_access_url"&gt;link&lt;/a&gt;</dcterms:references>
    <reuse xml:lang="en">A</reuse>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/locn#geometry">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">geometry</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/prov#Activity">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Activity</skos:prefLabel>
    <skos:definition xml:lang="en">An activity is something that occurs over a period of time and acts upon or with entities; it may include consuming, processing, transforming, modifying, relocating, using, or generating entities.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2006/vcard/ns#Kind">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Kind</skos:prefLabel>
    <skos:definition xml:lang="en">A description following the vCard specification, e.g. to provide telephone number and e-mail address for a contact point.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Document">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Document</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/creator">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">creator</skos:prefLabel>
    <skos:definition xml:lang="en">An entity responsible for producing the dataset.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/identifier">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <skos:prefLabel xml:lang="en">identifier</skos:prefLabel>
    <skos:definition xml:lang="en">The main identifier for the Dataset, e.g. the URI or other unique identifier in the context of the Catalogue. Usage Note: The use of persistent dereferenceable URIs is mandatory in this profile. (dcat:Dataset)</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/Agent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Agent</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#CatalogRecord">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Catalog Record</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#Distribution">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Distribution</skos:prefLabel>
    <skos:definition xml:lang="en">A physical embodiment of the Dataset in a particular format.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.europa.eu/eli/ontology#LegalResource">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Legal Resource</skos:prefLabel>
    <skos:definition xml:lang="en">This class represents the legislation, policy or policies that lie behind the Rules that govern the service. Usage Note The definition and properties of the Legal Resource class are aligned with the ontology included in "Council conclusions inviting the introduction of the European Legislation Identifier (ELI)". For describing the attributes of a Legal Resource (labels, preferred labels, alternative labels, definition, etc.) we refer to the ELI ontology. In this data specification the use is restricted to instances of this class that follow the ELI URI guidelines</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/modified">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <skos:prefLabel xml:lang="en">modified</skos:prefLabel>
    <skos:definition xml:lang="en">The most recent date on which the Dataset was changed or modified. (dcat:Dataset) The most recent date on which the Distribution was changed or modified. (dcat:Distribution)</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#compressFormat">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">compress Format</skos:prefLabel>
    <skos:definition xml:lang="en">compressed form, e.g. to reduce the size of the downloadable file. Usage Note: It SHOULD be expressed using a media type as defined in the official register of media types managed by IANA.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#spatialResolutionInMeters">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <skos:prefLabel xml:lang="en">spatial Resolution In Meters</skos:prefLabel>
    <skos:definition xml:lang="en">The minimum spatial separation resolvable in a dataset, measured in metres. (dcat:Dataset) The minimum spatial separation resolvable in a dataset distribution, measured in meters. (dcat:Distribution)</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/publisher">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">publisher</skos:prefLabel>
    <skos:definition xml:lang="en">An entity (organisation) responsible for making the Dataset available. Usage: DCAT-AP-LU provides additional usage guidance regarding vocabularies for publishers. An entity (organisation) responsible for making the Dataset available. Usage note: DCAT-AP-LU provides additional usage guidance regarding vocabularies for publishers</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://xmlns.com/foaf/0.1/primaryTopic">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">primary Topic</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/adms#versionNotes">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <skos:prefLabel xml:lang="en">version Notes</skos:prefLabel>
    <skos:definition xml:lang="en">A description of the differences between this version and a previous version of the Dataset. (dcat:Dataset)</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/prov#Attribution">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Attribution</skos:prefLabel>
    <skos:definition xml:lang="en">Attribution is the ascribing of an entity to an agent.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/adms#identifierX">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">identifier X</skos:prefLabel>
    <skos:definition xml:lang="en">A secondary identifier of the Dataset.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/PeriodOfTime">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Period Of Time</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/Standard">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Standard</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/Location">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Location</skos:prefLabel>
    <skos:definition xml:lang="en">A temporal period that the Dataset covers.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/relation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <skos:prefLabel xml:lang="en">relation</skos:prefLabel>
    <skos:definition xml:lang="en">A related resource. (dcat:Dataset)</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#DatasetSeries">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <skos:prefLabel xml:lang="en">Dataset Series</skos:prefLabel>
    <skos:definition xml:lang="en">A collection of datasets that are published separately, but share some characteristics that group them. Usage Note It is recommended to avoid Dataset Series without a dataset in the collection. Therefore at least one Dataset should refer to a Dataset Series using the property in series (dcat:inSeries).</skos:definition>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dcat#Resource"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/spatial">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">spatial</skos:prefLabel>
    <skos:definition xml:lang="en">A geographic region that is covered by the Dataset. A geographic region that is covered by the Dataset Series</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#service">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">service</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://purl.org/dc/terms/accrualPeriodicity">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <skos:prefLabel xml:lang="en">accrual Periodicity</skos:prefLabel>
    <skos:definition xml:lang="en">The frequency at which the Dataset is updated.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/2006/time#hasBeginning">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <skos:prefLabel xml:lang="en">has Beginning</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dcat#endDate">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <skos:prefLabel xml:lang="en">end Date</skos:prefLabel>
  </rdf:Description>
</rdf:RDF>
