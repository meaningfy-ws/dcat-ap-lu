    dcterms:publisher "https://mindig_lu.gitlab.io/DCAT-AP-LU" ;
    dcterms:title "DCAT-AP-LU"@en ;
    vann:preferredNamespacePrefix "dal" ;
    vann:preferredNamespaceUri "https://mindig_lu.gitlab.io/DCAT-AP-LU/#" ;
    rdfs:comment "This version is automatically generated from dcat_ap_lu_CM.xml on 2025-11-25" ;
    rdfs:seeAlso <https://mindig_lu.gitlab.io/DCAT-AP-LU/> ;
