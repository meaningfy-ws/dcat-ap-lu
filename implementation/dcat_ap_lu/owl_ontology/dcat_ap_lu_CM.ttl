rdf:PlainLiteral a rdfs:Datatype ;
    skos:prefLabel "Plain Literal"@en .

rdfs:Literal a rdfs:Datatype ;
    skos:definition "A literal value such as a string or integer. Literals may be typed, e.g. as a date according to xsd:date. Literals that contain human-readable text have an optional language tag as defined by BCP 4715 [[RFC5646]]."@en ;
    skos:prefLabel "Literal"@en .

rdfs:Resource a rdfs:Datatype ;
    skos:prefLabel "Resource"@en .

xsd:date a rdfs:Datatype ;
    skos:prefLabel "date"@en .

xsd:dateTime a rdfs:Datatype ;
    skos:definition "Object with integer-valued year, month, day, hour and minute properties, a decimal-valued second property, and a boolean timezoned property."@en ;
    skos:prefLabel "date Time"@en .

xsd:decimal a rdfs:Datatype ;
    skos:definition "Decimal represents a subset of the real numbers, which can be represented by decimal numerals. The value space of decimal is the set of numbers that can be obtained by multiplying an integer by a non-positive power of ten, i.e., expressible as i &#215; 10^-n where i and n are integers and n ≥ 0."@en ;
    skos:prefLabel "decimal"@en .

xsd:duration a rdfs:Datatype ;
    skos:definition "Duration represents a duration of time. The &#183;value space&#183; of duration is a six-dimensional space where the coordinates designate the Gregorian year, month, day, hour, minute, and second components defined in &#167; 5.5.3.2 of [[ISO8601-2]], respectively."@en ;
    skos:prefLabel "duration"@en .

xsd:gYear a rdfs:Datatype ;
    skos:prefLabel "g Year"@en .

xsd:gYearMonth a rdfs:Datatype ;
    skos:prefLabel "g Year Month"@en .

xsd:integer a rdfs:Datatype ;
    skos:prefLabel "integer"@en .

xsd:nonNegativeInteger a rdfs:Datatype ;
    skos:definition "Number derived from integer by setting the value of minInclusive to be 0."@en ;
    skos:prefLabel "non Negative Integer"@en .

xsd:string a rdfs:Datatype ;
    skos:prefLabel "string"@en .

time:Instant a rdfs:Datatype ;
    skos:definition "A temporal entity with zero extent or duration."@en ;
    skos:prefLabel "Instant"@en .

:TemporalLiteral a rdfs:Datatype ;
    rdfs:isDefinedBy core-res:core ;
    skos:definition "rdfs:Literal encoded using the relevant [[ISO8601-2]] Date and Time compliant string and typed using the appropriate XML Schema datatype (xsd:gYear, xsd:gYearMonth, xsd:date, or xsd:dateTime)."@en ;
    skos:prefLabel "Temporal Literal"@en .

core-res:core a owl:Ontology ;
    rdfs:label """This artefact provides the definitions for the DCAT-AP-LU.
        This artefact excludes the restrictions.
        The DCAT-AP-LU describes objects and concepts, with definitions, attributes and relationships which are used for sharing information about Catalogues containing Datasets and Data Services descriptions in Europe.
        The provision of these concepts provides the basis for a common understanding of the domain for all stakeholders ensuring the quality of data exchange and transparency."""@en ;
    dcterms:created "2025-12-15"^^xsd:date ;
    dcterms:description """This artefact provides the definitions for the DCAT-AP-LU.
        This artefact excludes the restrictions.
        DCAT-AP-LU is a Luxembourg-specific extension of the DCAT-AP profile used for sharing information about Catalogues containing Datasets and Data Services descriptions in Europe. It was created as part of the national implementation of the Data Governance Act (DGA) to support the creation of a single national dataset catalogue for all public sector data. The DCAT-AP-LU reuses large parts of the DCAT-AP-v3.0 standard, with a set of minor adaptations to support specific requirements for Luxembourg."""@en ;
    dcterms:issued "2025-12-16"^^xsd:date ;
