dcterms:title a owl:FunctionalProperty ;
    rdfs:domain dcat:Dataset ;
    rdfs:range :char .

dcat:Dataset rdfs:subClassOf [ a owl:Restriction ;
            owl:onDataRange :char ;
            owl:onProperty dcterms:title ;
            owl:qualifiedCardinality 1 ] .

