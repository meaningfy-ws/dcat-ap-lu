    rdfs:range odrl:Policy .

foaf:primaryTopic a owl:FunctionalProperty ;
    rdfs:domain dcat:CatalogRecord ;
    rdfs:range dcat:Resource .

dcterms:Location rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass locn:Geometry ;
            owl:onProperty locn:geometry ] .

dcterms:issued rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Dataset dcat:Distribution ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( :TemporalLiteral xsd:dateTime ) ] .

dcterms:license rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Catalog dcat:Distribution ) ] ;
    rdfs:range dcterms:LicenseDocument .

dcterms:modified rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Dataset dcat:Distribution ) ] ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( :TemporalLiteral xsd:dateTime ) ] .

dcterms:rights rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Dataset dcat:Distribution ) ] ;
    rdfs:range dcterms:RightsStatement .

dcterms:type rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Dataset dcterms:LicenseDocument foaf:Agent ) ] ;
    rdfs:range skos:Concept .

dcat:CatalogRecord rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass dcat:Resource ;
            owl:onProperty foaf:primaryTopic ;
            owl:qualifiedCardinality 1 ] .

dcterms:publisher a owl:FunctionalProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Catalog dcat:Dataset dcat:DatasetSeries ) ] ;
    rdfs:range foaf:Agent .

dcterms:PeriodOfTime rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange time:Instant ;
            owl:onProperty time:hasEnd ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange time:Instant ;
            owl:onProperty time:hasBeginning ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange :TemporalLiteral ;
            owl:onProperty dcat:endDate ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange :TemporalLiteral ;
            owl:onProperty dcat:startDate ] .

dcat:Catalog rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass foaf:Agent ;
            owl:onProperty dcterms:publisher ;
            owl:qualifiedCardinality 1 ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
