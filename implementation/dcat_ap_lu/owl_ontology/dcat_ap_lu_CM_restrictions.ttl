            owl:unionOf ( dcat:Dataset dcat:Distribution ) ] ;
    rdfs:range xsd:decimal .

dcat:startDate rdfs:domain dcterms:PeriodOfTime ;
    rdfs:range :TemporalLiteral .

dcat:temporalResolution rdfs:domain [ a owl:Class ;
            owl:unionOf ( dcat:Dataset dcat:Distribution ) ] ;
