        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcterms:MediaType ;
            owl:onProperty dcat:compressFormat ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange xsd:nonNegativeInteger ;
            owl:onProperty dcat:byteSize ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcterms:LicenseDocument ;
            owl:onProperty dcterms:license ] .

dcat:Dataset rdfs:subClassOf [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onClass dcterms:Frequency ;
            owl:onProperty dcterms:accrualPeriodicity ],
        [ a owl:Restriction ;
            owl:maxQualifiedCardinality 1 ;
            owl:onDataRange :TemporalLiteral ;
            owl:onProperty dcterms:issued ],
        [ a owl:Restriction ;
