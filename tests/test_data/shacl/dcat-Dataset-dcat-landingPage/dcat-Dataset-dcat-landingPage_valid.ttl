@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex:   <http://example.org/> .

ex:dataset-001 a dcat:Dataset ;
    dct:identifier "dataset-123" ;
    dct:title "Dataset with Landing Page"@en ;
    dct:description "A proper dataset with a valid landing page."@en ;
    dcat:distribution ex:dist-1 ;
    dcat:landingPage ex:landingPage1 .

ex:landingPage1 a foaf:Document ;
    foaf:homepage <http://example.org/landing-page> .

ex:dist-1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .