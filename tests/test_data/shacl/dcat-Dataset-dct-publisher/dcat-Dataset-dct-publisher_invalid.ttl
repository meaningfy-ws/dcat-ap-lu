#  Dataset 2: Too many publishers
ex:dataset-invalid-too-many a dcat:Dataset ;
    dct:identifier "dataset-too-many" ;
    dct:title "Too Many Publishers" ;
    dct:description "Violates maxCount = 1 for dct:publisher" ;
    dcat:distribution ex:dist-2 ;
    dct:publisher ex:agent1, ex:agent2 .

ex:dist-2 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-002> .


ex:agent1 a foaf:Agent ;
    foaf:name "Publisher One" .

ex:agent2 a foaf:Agent ;
    foaf:name "Publisher Two" .

#  Dataset 3: Circular reference
ex:dataset-invalid-circular a dcat:Dataset ;
    dct:identifier "dataset-circular" ;
    dct:title "Circular Publisher" ;
    dct:description "Publisher points back to dataset (circular)" ;
    dcat:distribution ex:dist-3 ;
    dct:publisher ex:dataset-invalid-circular .

ex:dist-3 a dcat:Distribution ;
     dcat:accessURL <http://example.org/download/dataset-003> .
