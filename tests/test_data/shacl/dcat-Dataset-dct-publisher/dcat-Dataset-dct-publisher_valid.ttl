@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex:   <http://example.org/> .

ex:dataset-001 a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Valid Publisher Dataset" ;
    dct:description "Dataset with a valid publisher." ;
    dcat:distribution ex:dist1 ;
    dct:publisher ex:publisher1 .

ex:dist1 a dcat:Distribution ;
   dcat:accessURL <http://example.org/download/dataset-001> .

ex:publisher1 a foaf:Agent ;
    foaf:name "Publishing Org" .
