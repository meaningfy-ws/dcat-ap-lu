@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:   <http://example.org/> .

# Required supporting nodes
ex:rights-1 a dct:RightsStatement .
ex:publisher-1 a foaf:Agent ; foaf:name "Publishing Org"@en .

# Dataset (meets required identifier/title/description/distribution/publisher/rights)
ex:dataset-001 a dcat:Dataset ;
  dct:identifier  "dataset-001" ;
  dct:title       "Valid Publisher Dataset"@en ;
  dct:description "Dataset with a valid publisher."@en ;
  dct:publisher   ex:publisher-1 ;
  dct:rights      ex:rights-1 ;
  dcat:distribution ex:dist-001 .

# Distribution (meets required accessURL + rights)
ex:dist-001 a dcat:Distribution ;
  dct:rights ex:rights-1 ;
  dcat:accessURL "http://example.org/download/dataset-001"^^rdfs:Resource .
