@prefix dcat:   <http://www.w3.org/ns/dcat#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:     <http://example.org/> .

# Required supporting node for Distribution base constraints
ex:rights-1 a dct:RightsStatement .

ex:distribution1 a dcat:Distribution ;
    dct:rights ex:rights-1 ;
    dcat:accessURL "http://example.org/download/dataset-001"^^rdfs:Resource ;
    dct:language [
        a dct:LinguisticSystem ;
        rdfs:seeAlso <https://publications.europa.eu/resource/authority/language/ENG>
    ] .
