@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:   <http://example.org/> .

# Required supporting nodes
ex:publisher-1 a foaf:Agent ;
  foaf:name "Example publisher"@en .

ex:rights-1 a dct:RightsStatement .

# WRONG: this is NOT a skos:Concept (so it should violate sh:class skos:Concept on dct:type)
ex:not-a-concept a foaf:Agent ;
  foaf:name "Not a Concept"@en .

# Dataset: everything required is present; only dct:type is wrong
ex:dataset-invalid a dcat:Dataset ;
  dct:identifier  "dataset-002" ;
  dct:title       "Invalid Dataset with Type"@en ;
  dct:description "dct:type points to a non skos:Concept."@en ;
  dct:publisher   ex:publisher-1 ;
  dct:rights      ex:rights-1 ;
  dct:type        ex:not-a-concept ;
  dcat:distribution ex:dist-invalid .

# Distribution: valid
ex:dist-invalid a dcat:Distribution ;
  dct:rights ex:rights-1 ;
  dcat:accessURL "http://example.org/download/dataset-002"^^rdfs:Resource .
