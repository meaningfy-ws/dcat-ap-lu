@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex:   <http://example.org/> .

ex:dataset-valid a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Valid Dataset with Type" ;
    dct:description "Describes Dataset with Type." ;
    dct:type ex:archived-data ;
    dcat:distribution ex:dist-1 .

ex:archived-data a skos:Concept ;
    skos:prefLabel "Archived Data"@en .

ex:dist-1 a dcat:Distribution ;
     dcat:accessURL <http://example.org/download/dataset-001> .
