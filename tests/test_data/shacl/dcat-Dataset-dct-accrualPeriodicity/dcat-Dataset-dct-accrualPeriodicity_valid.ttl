@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:   <http://example.org/> .

# Required supporting nodes
ex:publisher-1 a foaf:Agent ;
  foaf:name "Example publisher"@en .

ex:rights-1 a dct:RightsStatement .

# Dataset (all required fields + valid accrualPeriodicity)
ex:dataset-valid a dcat:Dataset ;
  dct:identifier  "dataset-003" ;
  dct:title       "Valid Dataset"@en ;
  dct:description "Dataset with valid accrual periodicity."@en ;
  dct:publisher   ex:publisher-1 ;
  dct:rights      ex:rights-1 ;
  dcat:distribution ex:dist1 ;
  dct:accrualPeriodicity ex:freq-annual .

# Correct class
ex:freq-annual a dct:Frequency ;
  dct:title "Annual"@en .

# Distribution meets your required constraints
ex:dist1 a dcat:Distribution ;
  dct:rights ex:rights-1 ;
  dcat:accessURL "http://example.org/download/dataset-003"^^rdfs:Resource .
