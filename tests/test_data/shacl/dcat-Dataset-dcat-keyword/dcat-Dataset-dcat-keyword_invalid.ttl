@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:   <http://example.org/> .

# Required supporting nodes (avoid noise)
ex:publisher-1 a foaf:Agent ;
  foaf:name "Example publisher"@en .

ex:rights-1 a dct:RightsStatement .

ex:dataset-invalid a dcat:Dataset ;
  dct:identifier  "dataset-invalid" ;
  dct:title       "Invalid Dataset"@en ;
  dct:description "This dataset violates keyword rules only."@en ;
  dct:publisher   ex:publisher-1 ;
  dct:rights      ex:rights-1 ;
  dcat:distribution ex:distribution1 ;

  # Violation 1: keyword is NOT a literal (IRI instead)
  dcat:keyword ex:kw1 ;

  # Violation 2: keyword literal WITHOUT language tag (if lang is required)
  dcat:keyword "biodiversity" .

ex:distribution1 a dcat:Distribution ;
  dct:rights ex:rights-1 ;
  dcat:accessURL "http://example.org/download/dataset-002"^^rdfs:Resource .

ex:kw1 a foaf:Document .
