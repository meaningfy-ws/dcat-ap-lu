@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .


ex:rights-1 a dcterms:RightsStatement .

ex:distribution1 a dcat:Distribution ;
    dcterms:rights ex:rights-1 ;
  dcat:accessURL "http://example.org/download/dataset-001"^^rdfs:Resource ;
    dcat:compressFormat ex:gzipFormat .

ex:gzipFormat a dcterms:MediaTyp;
    dcterms:title "GZIP" .

ex:distribution2 a dcat:Distribution ;
    dcterms:rights ex:rights-1 ;
  dcat:accessURL "http://example.org/download/dataset-002"^^rdfs:Resource ;
    dcat:compressFormat ex:zipFormat, ex:gzFormat .
ex:zipFormat a dcterms:MediaType ;
    dcterms:title "zip" .

ex:gzFormat a dcterms:MediaType ;
    dcterms:title "GZ" .

ex:distribution3 a dcat:Distribution ;
    dcterms:rights ex:rights-1 ;
  dcat:accessURL "http://example.org/download/dataset-003"^^rdfs:Resource ;
    dcat:compressFormat ex:circularFormat .

# circular reference
ex:circularFormat a dcterms:MediaType ;
    dcat:compressFormat ex:distribution3 ;
    dcterms:identifier "application/x-circular" .