@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix dpv:  <https://w3id.org/dpv#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex:   <http://example.org/> .

# Invalid Dataset with multiple issues
ex:dataset-invalid a dcat:Dataset ;
    dct:identifier "dataset-999" ;
    dct:title "Invalid Dataset" ;
    dct:description "hasData";


#    #  Wrong class (foaf:Agent instead of dpv:Data)
#    dpv:hasData ex:notData1 ;

        #  Circular reference
    dpv:hasData ex:dataset-invalid ;

    dcat:distribution ex:dist-1 .

ex:notData1 a foaf:Agent ;
    foaf:name "Wrong Class" .

ex:dist-1 a dcat:Distribution ;
     dcat:accessURL <http://example.org/download/dataset-001> .
