@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix ex:   <http://example.org/> .

ex:validDataset a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Example DCAT Dataset"@en ;
    dct:description "A minimal dataset with valid spatial resolution."@en ;
    dcat:distribution ex:dist-1 ;
    dcat:spatialResolutionInMeters "10.5"^^xsd:decimal .
ex:dist-1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .