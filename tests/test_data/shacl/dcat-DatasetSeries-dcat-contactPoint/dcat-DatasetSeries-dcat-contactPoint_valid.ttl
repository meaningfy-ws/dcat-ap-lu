@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix ex: <http://example.org/> .

ex:datasetSeries-valid a dcat:DatasetSeries ;
    dct:identifier "ds-series-001" ;
    dct:title "Valid Dataset Series" ;
    dct:description "This dataset series has valid contact point." ;
    dcat:contactPoint ex:contact-valid .

ex:contact-valid a vcard:Kind ;
    vcard:fn "Dataset Series Support Team" ;
    vcard:hasEmail <mailto:support@example.org> .