@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix spdx: <http://spdx.org/rdf/terms#> .
@prefix ex: <http://example.org/> .

ex:distribution1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> ;
     spdx:checksum ex:checksum-valid .

ex:checksum-valid a spdx:Checksum ;
    spdx:algorithm "SHA-256" ;
    spdx:checksumValue "abc1234567890def" .
