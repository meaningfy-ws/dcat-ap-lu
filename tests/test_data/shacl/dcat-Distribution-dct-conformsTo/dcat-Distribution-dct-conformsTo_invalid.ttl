@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.org/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

ex:rights-1 a dcterms:RightsStatement .

ex:distribution1 a dcat:Distribution ;
    dcterms:rights ex:rights-1 ;
    dcat:accessURL <http://example.org/download/dataset-001> ;
    dcterms:conformsTo ex:notAStandard .

ex:notAStandard a rdfs:Resource ;
    rdfs:label "Some Spec"@en .

###########  INVALID: circular reference ###########
ex:distribution2 a dcat:Distribution ;
    dcterms:rights ex:rights-1 ;
    dcat:accessURL "http://example.org/download/dataset-001"^^rdfs:Resource ;
    dcterms:conformsTo ex:loop .
ex:loop a dcterms:Standard ;
    dcterms:conformsTo ex:distribution2;
    rdfs:label "Circular Spec Ref"@en .
