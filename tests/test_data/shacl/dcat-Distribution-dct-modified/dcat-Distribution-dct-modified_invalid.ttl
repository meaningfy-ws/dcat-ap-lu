@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:   <http://example.org/> .

############################################################
# Shared required node
############################################################
ex:rights-1 a dct:RightsStatement .

############################################################
# Invalid 1: WRONG DATATYPE for dct:modified
############################################################
ex:distribution-invalid-datatype a dcat:Distribution ;
    dct:rights ex:rights-1 ;
    dcat:accessURL "http://example.org/download/dataset-001"^^rdfs:Resource ;
    dct:modified "2023-10-12T12:00:00Z" .   # ‚ùå not xsd:dateTime

############################################################
# Invalid 2: TOO MANY VALUES for dct:modified (maxCount = 1)
############################################################
ex:distribution-invalid-maxcount a dcat:Distribution ;
    dct:rights ex:rights-2 ;
    dcat:accessURL "http://example.org/download/dataset-002"^^rdfs:Resource ;
    dct:modified "2023-01-01T00:00:00Z"^^xsd:dateTime ,
                 "2024-01-01T00:00:00Z"^^xsd:dateTime .
