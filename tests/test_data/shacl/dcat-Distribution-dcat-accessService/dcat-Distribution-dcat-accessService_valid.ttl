@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.org/> .

ex:rights-1 a dcterms:RightsStatement .

# Distribution using accessService
ex:distribution1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/data> ;
    dcterms:rights ex:rights-1 ;
    dcat:accessService ex:dataservice1 .

# The DataService referred to
ex:dataservice1 a dcat:DataService ;
    dcterms:title "API for accessing the dataset"@en .
