@prefix dcat:   <http://www.w3.org/ns/dcat#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix ex:     <http://example.org/> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .

############################################################
# INVALID Dataset — dct:rights (exactly 3 violations)
############################################################

ex:dataset-invalid a dcat:Dataset ;
    dct:identifier "dataset-002" ;
    dct:title "Invalid Dataset" ;
    dcat:distribution ex:dist1 ;

    # Violation 1 — wrong class (not a RightsStatement)
    dct:rights ex:wrong1 ;

    # Violation 2 — second wrong-class entry → maxCount broken
    dct:rights ex:wrong2 .

# WRONG CLASS OBJECTS (no Dataset typing!)
ex:wrong1 a foaf:Agent .
ex:wrong2 a foaf:Person .

############################################################
# Violation 3 — accessURL wrong datatype
############################################################

ex:dist1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-002> .
