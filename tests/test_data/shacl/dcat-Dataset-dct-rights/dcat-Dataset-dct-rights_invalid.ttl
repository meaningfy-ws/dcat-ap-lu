@prefix dcat:   <http://www.w3.org/ns/dcat#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix ex:     <http://example.org/> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .

############################################################
# INVALID Dataset — dct:accessRights violations
############################################################

ex:dataset-invalid a dcat:Dataset ;
    dct:identifier "dataset-002" ;
    dct:title "Invalid Dataset"@en ;
    dct:description "A dataset with invalid access rights."@en ;
    dcat:distribution ex:dist1 ;

    # ❌ Violation 1 — wrong class (must be dct:RightsStatement)
    dct:accessRights ex:notRights1 ;

    # ❌ Violation 2 — extra value (maxCount=1), also wrong class
    dct:accessRights ex:notRights2 .

# wrong classes
ex:notRights1 a foaf:Agent .
ex:notRights2 a dcat:Dataset .

############################################################
# INVALID Distribution — accessURL violates datatype constraint
############################################################

ex:dist1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-002> .
# ❌ violates sh:datatype rdfs:Resource (expects literal)
