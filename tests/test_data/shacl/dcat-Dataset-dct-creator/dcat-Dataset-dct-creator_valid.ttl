@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex:   <http://example.org/> .

ex:dataset-001 a dcat:Dataset ;
    dct:identifier "dataset-007" ;
    dct:title "Valid Creator Dataset"@en ;
    dct:description "This dataset has a valid creator."@en ;
    dcat:distribution ex:dist1 ;
    dct:creator ex:agent1 .

ex:agent1 a foaf:Agent ;
    foaf:name "Example Creator Org"@en .
ex:dist1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .