@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix odrl: <http://www.w3.org/ns/odrl/2/> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:   <http://example.org/> .

# Required by your Distribution shapes
ex:rights-1 a dct:RightsStatement .

ex:distribution1 a dcat:Distribution ;
    dct:rights ex:rights-1 ;
    dcat:accessURL "http://example.org/download/dataset-001"^^rdfs:Resource ;
    dct:title "Valid distribution with policy"@en ;
    odrl:hasPolicy ex:policy1 .

ex:policy1 a odrl:Policy ;
    dct:title "Policy about usage rights"@en .
