@prefix : <vscode-remote://wsl%2Bubuntu-24.04/home/schiv/dcat-ap-lu/tests/test_data/shacl/dcat-Distribution-dcatap-availability/dcat-Distribution-dcatap-availability_invalid.ttl#> .
@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcatap: <http://data.europa.eu/r5r/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.org/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .


# wrong class
ex:distribution1 a dcat:Distribution ;
    dcterms:rights ex:rights-1 ;
    dcat:accessURL <http://example.org/download/dataset-001> ;
    dcatap:availability ex:availability1 .

ex:availability1 a skos:Concepto ;
    skos:prefLabel "Concept"@en .

# too many values
ex:distribution2 a dcat:Distribution ;
     dcterms:rights ex:rights-2 ;
    dcat:accessURL <http://example.org/download/dataset-001> ;
    dcatap:availability ex:availability2a , ex:availability2b .

ex:availability2a a skos:Concept ;
    skos:prefLabel "Concept 2A"@en .

ex:availability2b a skos:Concept ;
    skos:prefLabel "Concept 2B"@en .

# circular reference
ex:distribution3 a dcat:Distribution ;
    dcterms:rights ex:rights-1 ;
    dcat:accessURL <http://example.org/download/dataset-001> ;
    dcatap:availability ex:availability3 .

ex:availability3 a skos:Concept ;
    dcatap:availability ex:distribution3 ;
    skos:prefLabel "Concept 3"@en .
