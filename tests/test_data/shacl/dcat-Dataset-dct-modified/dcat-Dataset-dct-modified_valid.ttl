@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .

ex:dataset-001 a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Valid Dataset" ;
    dct:description "A valid dataset with correct modified date." ;
    dcat:distribution ex:dist1 ;
    dct:modified "2024-12-31T12:00:00"^^xsd:dateTime .

ex:dist1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .
