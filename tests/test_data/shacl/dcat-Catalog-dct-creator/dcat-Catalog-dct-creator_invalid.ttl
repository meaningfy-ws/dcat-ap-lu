@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex: <http://example.org/> .

ex:publisher1 a foaf:Agent .

################################################
# 1. Too many creators (maxCount violation)
################################################
ex:catalog2 a dcat:Catalog ;
    dcterms:publisher ex:publisher1 ;       # REQUIRED
    dcterms:creator ex:agent2a, ex:agent2b .

ex:agent2a a foaf:Agent .
ex:agent2b a foaf:Agent .

################################################
# 2. Wrong class (class violation)
################################################
ex:catalog3 a dcat:Catalog ;
    dcterms:publisher ex:publisher1 ;      # REQUIRED
    dcterms:creator ex:wrongClass .

ex:wrongClass a rdfs:Class .

################################################
# 3. Circular reference ONLY (SPARQL violation)
################################################
ex:catalog4 a dcat:Catalog, foaf:Agent ;   # Add foaf:Agent to avoid class violation
    dcterms:publisher ex:publisher1 ;
    dcterms:creator ex:catalog4 .
