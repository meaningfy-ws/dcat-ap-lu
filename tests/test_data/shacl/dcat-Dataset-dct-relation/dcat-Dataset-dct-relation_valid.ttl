@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .

ex:dataset-001 a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Dataset with valid relation" ;
    dct:description "A dataset that links to a related resource." ;
    dcat:distribution ex:dist-1 ;
    dct:relation <http://example.org/related/resource> .

ex:dist-1 a dcat:Distribution ;
   dcat:accessURL <http://example.org/download/dataset-001> .
