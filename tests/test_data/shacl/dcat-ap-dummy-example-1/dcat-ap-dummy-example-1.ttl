@prefix dcat:   <http://www.w3.org/ns/dcat#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix skos:   <http://www.w3.org/2004/02/skos/core#> .
@prefix dcatap: <http://data.europa.eu/r5r/> .
@prefix :       <https://example.org/dcat-ap-lu-examples/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

#################################################################
# Catalog
#################################################################
:Catalog-full
  a dcat:Catalog;
  dct:language [
    a dct:tLinguisticSystem ;
    rdfs:seeAlso <http://publications.europa.eu/resource/authority/language/ENG>
  ] ;
dct:publisher [
    a foaf:Agent, foaf:Organization ;
    foaf:name "meaningfy"@eng ;
  ] .
#################################################################
# dcat:CatalogRecord
#################################################################
:CatalogRecord-full
  a dcat:CatalogRecord ;
  dct:language [
    a dct:tLinguisticSystem ;
    rdfs:seeAlso <https://publications.europa.eu/resource/authority/language/ENG>
  ] ;
    foaf:primaryTopic :dataset-1 .

#################################################################
# Dataset Series (mandatory dct:title and dct:description)
#################################################################

:series-001
    a dcat:DatasetSeries ;
    dct:title "Example Dataset Series"@en ;
    dct:description "This series groups related datasets including different versions."@en ;
    dct:publisher [
    a foaf:Agent, foaf:Organization ;
    foaf:name "team"@eng
  ];
    dct:spatial [
    a dct:Location ;
    rdfs:seeAlso <https://publications.europa.eu/resource/authority/country/LUX> ;
    skos:prefLabel "Luxembourg"@eng
  ].

:dataset-1
    a                   dcat:Dataset ;
    dct:identifier      "dataset-001" ;
    dct:title           "Example DCAT Dataset" ;
    dct:description     "A minimal dataset with only mandatory properties." ;
     dct:rights [ a dct:RightsStatement ;
        rdfs:seeAlso <https://publications.europa.eu/resource/authority/access-right/PUBLIC> ;
        skos:prefLabel "Public"@en ] ;
        dct:publisher [ a foaf:Agent ; foaf:name "Publishing Org"@en ] ;

    dcat:distribution   :dist-1 .



:dist-1
  a dcat:Distribution ;

  dcat:accessURL <http://example.org/dataset-001.json> ;

  # mediaType must be a dcterms:MediaType
  dcat:mediaType [
    a dct:MediaType ;
    owl:sameAs <https://www.iana.org/assignments/media-types/application/json>
  ] ;

  # packageFormat must be a dcterms:MediaType
  dcat:packageFormat [
    a dct:MediaType ;
    owl:sameAs <https://www.iana.org/assignments/media-types/application/y3gppHal+json>
  ] ;
dct:rights [ a dct:RightsStatement ;
        rdfs:seeAlso <https://publications.europa.eu/resource/authority/access-right/PUBLIC> ;
        skos:prefLabel "Public"@en ] ;
  # availability must be a skos:Concept
dcatap:availability [
    a skos:Concept ;
   owl:sameAs <http://publications.europa.eu/resource/authority/planned-availability/TEMPORARY> ;
    skos:prefLabel "Temporary"@eng
  ] .