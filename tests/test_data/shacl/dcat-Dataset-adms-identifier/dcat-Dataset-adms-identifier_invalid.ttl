@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/> .
@prefix dct:    <http://purl.org/dc/terms/> .

#  Violation 1: Not an instance of adms:Identifier
ex:dataset2 a dcat:Dataset ;
    adms:identifier ex:notAnIdentifier ;
     dct:title           "Example DCAT Dataset" ;
    dct:description     "A minimal dataset with only mandatory properties." ;
    dct:identifier      "dataset-001" ;
    dcat:distribution   ex:distribution1 .

ex:notAnIdentifier a skos:Concep .

#  Violation 2: Circular reference (identifier points back to dataset)
ex:dataset3 a dcat:Dataset ;
    adms:identifier ex:loop ;
    dct:title           "Example DCAT Dataset" ;
    dct:description     "A minimal dataset with only mandatory properties." ;
    dct:identifier      "dataset-001" ;
    dcat:distribution   ex:distribution2 .


ex:loop a adms:Identifier ;
         adms:identifier ex:dataset3 .

ex:distribution1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .

ex:distribution2 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-002> .