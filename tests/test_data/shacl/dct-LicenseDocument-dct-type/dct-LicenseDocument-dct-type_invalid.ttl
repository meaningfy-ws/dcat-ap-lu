@prefix dct: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ex: <http://example.org/> .

# Invalid:
# - dct:type refers to a resource with the wrong class (foaf:Document)
# - Circular reference (license-invalid <-> licensetype-wrong)

ex:license-invalid a dct:LicenseDocument ;
    dct:type ex:licensetype-wrong .

ex:licensetype-wrong a foaf:Document ;  # Wrong class, should be skos:Concept
    dct:type ex:license-invalid .       # Circular reference
