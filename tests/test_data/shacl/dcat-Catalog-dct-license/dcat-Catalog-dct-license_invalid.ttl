@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:   <http://example.org/> .

ex:publisher1 a foaf:Agent .

### 1. Too many licenses (1 violation)
ex:catalog5 a dcat:Catalog ;
    dct:publisher ex:publisher1 ;
    dct:license ex:licA, ex:licB .

ex:licA a dct:LicenseDocument .
ex:licB a dct:LicenseDocument .

### 2. Wrong class (1 violation)
ex:catalog6 a dcat:Catalog ;
    dct:publisher ex:publisher1 ;
    dct:license ex:notLicense .

ex:notLicense a rdfs:Class .

### 3. Circular reference (1 violation ONLY)
ex:catalog7 a dcat:Catalog, dct:LicenseDocument ;
    dct:publisher ex:publisher1 ;
    dct:license ex:catalog7 .
