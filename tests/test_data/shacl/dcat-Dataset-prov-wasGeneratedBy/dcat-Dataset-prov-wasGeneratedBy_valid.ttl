@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix ex: <http://example.org/> .

ex:dataset-valid a dcat:Dataset ;
    dct:identifier "dataset-005" ;
    dct:title "Dataset with Valid wasGeneratedBy" ;
    dct:description "This dataset links to a valid prov:Activity." ;
    dcat:distribution ex:dist-1 ;
    prov:wasGeneratedBy ex:activity-1 .

ex:activity-1 a prov:Activity ;
    dct:description "Data processing activity for generating dataset." .

ex:dist-1 a dcat:Distribution ;
   dcat:accessURL <http://example.org/download/dataset-001> .

