@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .

ex:dataset-valid a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Example DCAT Dataset"@en ;
    dct:description "A dataset with a proper identifier."@en ;
    dcat:distribution ex:distribution1 .

ex:distribution1 a dcat:Distribution ;
    dct:title "CSV Export"@en ;
    dcat:accessURL <http://example.org/download/dataset-001> .
