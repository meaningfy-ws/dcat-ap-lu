@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:   <http://example.org/> .

# Shared required supporting nodes (avoid extra noise)
ex:publisher-1 a foaf:Agent ;
  foaf:name "Example publisher"@en .

ex:rights-1 a dct:RightsStatement .

# Valid distributions (avoid extra noise)
ex:dist-a a dcat:Distribution ;
  dct:rights ex:rights-1 ;
  dcat:accessURL "http://example.org/download/dataset-a"^^rdfs:Resource .

ex:dist-b a dcat:Distribution ;
  dct:rights ex:rights-1 ;
  dcat:accessURL "http://example.org/download/dataset-b"^^rdfs:Resource .

############################################################
# Invalid 1: MISSING dct:identifier (minCount violation)
############################################################
ex:dataset-missing-identifier a dcat:Dataset ;
  dct:title       "Dataset Without Identifier"@en ;
  dct:description "This dataset is missing the required identifier."@en ;
  dct:publisher   ex:publisher-1 ;
  dct:rights      ex:rights-1 ;
  dcat:distribution ex:dist-a .

############################################################
# Invalid 2: NON-LITERAL dct:identifier (datatype violation)
############################################################
ex:dataset-nonliteral-identifier a dcat:Dataset ;
  dct:identifier  ex:NonLiteralIdentifier ;
  dct:title       "Dataset With Non-literal Identifier"@en ;
  dct:description "Identifier must be a literal (rdfs:Literal) but is an IRI."@en ;
  dct:publisher   ex:publisher-1 ;
  dct:rights      ex:rights-1 ;
  dcat:distribution ex:dist-b .

ex:NonLiteralIdentifier a foaf:Document .
