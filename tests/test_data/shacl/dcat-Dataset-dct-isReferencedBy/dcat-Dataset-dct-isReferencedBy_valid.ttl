@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .

ex:dataset-valid a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Dataset with Citation"@en ;
    dct:description "This dataset is referenced by a publication."@en ;
    dct:isReferencedBy <https://example.org/pub/1> ;
    dcat:distribution ex:dist1 .

ex:dist1 a dcat:Distribution ;
    dct:title "CSV Version"@en ;
    dcat:accessURL <http://example.org/download/dataset-001> .