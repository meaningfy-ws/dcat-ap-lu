@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .

ex:dataset-001 a dcat:Dataset ;
    dct:identifier "dataset-999" ;
    dct:title "Broken Reference Dataset"@en ;
    dct:description "This dataset has an invalid isReferencedBy value."@en ;
    dct:publisher   ex:publisher-1 ;
    dct:rights      ex:rights-1 ;
    # Invalid reference (literal instead of URI)
    dct:isReferencedBy "This should be a URI not a literal." ;

    dcat:distribution ex:dist1 .

ex:dist1 a dcat:Distribution ;
    dct:title "Broken CSV"@en ;
    dcat:accessURL <http://example.org/download/dataset-001> .