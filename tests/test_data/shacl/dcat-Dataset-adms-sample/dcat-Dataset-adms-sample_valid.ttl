@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix adms: <http://www.w3.org/ns/adms#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex:   <http://example.org/> .

# Required supporting nodes (to satisfy Dataset and Distribution minCounts)
ex:publisher-1 a foaf:Agent ;
  foaf:name "Example publisher"@en .

ex:rights-1 a dct:RightsStatement .

ex:identifier1 a adms:Identifier ;
  skos:notation "ID-2025-001" .

# Dataset (all mandatory Dataset fields included)
ex:dataset1 a dcat:Dataset ;
    dct:identifier  "dataset-001" ;
    dct:title       "Example DCAT Dataset"@en ;
    dct:description "A minimal dataset with only mandatory properties."@en ;
    dct:publisher   ex:publisher-1 ;
    dct:rights      ex:rights-1 ;
    dcat:distribution ex:distribution1 ;
    adms:identifier ex:identifier1 ;
    adms:sample     ex:distribution1 .

# Distribution (all mandatory Distribution fields included)
ex:distribution1 a dcat:Distribution ;
    dct:rights ex:rights-1 ;
    dcat:accessURL "http://example.org/sample.csv"^^rdfs:Resource .
