@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.org/> .

ex:validDataset a dcat:Dataset ;
    dcterms:identifier "dataset-valid" ;
    dcterms:title "Valid Dataset" ;
    dcterms:description "This dataset has valid distribution(s)." ;
    dcat:distribution ex:distribution1, ex:distribution2 .

ex:distribution1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .
ex:distribution2 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-002> .