@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix ex: <http://example.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

ex:rights-1 a dcterms:RightsStatement .

# Case 1: Wrong datatype (plain literal, not typed)
ex:distribution1 a dcat:Distribution ;
    dcterms:rights ex:rights-1 ;
    dcat:accessURL <http://example.org/download/dataset-001> ;
    dcat:byteSize "123456" .

# this will trigger RDF parsing errors, validate with a validator like riot instead
# # Case 2: Invalid value for xsd:nonNegativeInteger (negative number)
# ex:distribution2 a dcat:Distribution ;
#    dcat:accessURL <http://example.org/download/dataset-002> ;
#    dcat:byteSize "-100"^^xsd:nonNegativeInteger .

# Case 3: Too many values (violates maxCount=1)
ex:distribution3 a dcat:Distribution ;
     dcat:accessURL <http://example.org/download/dataset-003> ;
     dcterms:rights ex:rights-1 ;
    dcat:byteSize "123456"^^xsd:nonNegativeInteger ;
    dcat:byteSize "7890"^^xsd:nonNegativeInteger .
