@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix ex:   <http://example.org/> .

ex:dataset-001 a dcat:Dataset ;
    dct:identifier "dataset-001" ;
    dct:title "Example Dataset With Valid Temporal Resolution"@en ;
    dct:description "Valid use of temporal resolution in xsd:duration."@en ;
    dcat:distribution ex:dist-1 ;
    dcat:temporalResolution "P1D"^^xsd:duration .  # P1D = 1 day
ex:dist-1 a dcat:Distribution ;
    dcat:accessURL <http://example.org/download/dataset-001> .