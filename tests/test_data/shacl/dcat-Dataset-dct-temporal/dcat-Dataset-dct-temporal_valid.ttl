@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd:  <http://www.w3.org/2001/XMLSchema#> .
@prefix ex:   <http://example.org/> .

# Required supporting nodes
ex:publisher-1 a foaf:Agent ;
  foaf:name "Example publisher"@en .

ex:rights-1 a dct:RightsStatement .

# Dataset (meets required identifier/title/description/distribution/publisher/rights)
ex:dataset1 a dcat:Dataset ;
  dct:identifier  "dataset-001" ;
  dct:title       "Dataset with valid temporal coverage"@en ;
  dct:description "Describes historical events."@en ;
  dct:publisher   ex:publisher-1 ;
  dct:rights      ex:rights-1 ;
  dcat:distribution ex:dist-1 ;
  dct:temporal    ex:period1 .

# Correct class for temporal
ex:period1 a dct:PeriodOfTime ;
  dcat:startDate "2023-01-01T00:00:00"^^xsd:dateTime ;
  dcat:endDate   "2023-12-31T23:59:59"^^xsd:dateTime .

# Distribution meets required rights + accessURL (typed literal to satisfy sh:datatype rdfs:Resource)
ex:dist-1 a dcat:Distribution ;
  dct:rights  ex:rights-1 ;
  dcat:accessURL "http://example.org/download/dataset-001"^^rdfs:Resource .
