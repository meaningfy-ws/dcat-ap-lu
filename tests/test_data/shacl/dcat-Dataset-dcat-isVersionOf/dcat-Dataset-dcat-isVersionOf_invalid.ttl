@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ex:   <http://example.org/> .

###############################################################
# INVALID — Violates sh:class dcat:Dataset (1 violation only)
###############################################################

ex:notADataset a rdfs:Class .

ex:dataset-invalid a dcat:Dataset ;
    dct:title "Invalid wrong class" ;
    dct:identifier "ds-invalid" ;
    dct:description "This ensures no other MinCount violations." ;
    dcat:isVersionOf ex:notADataset .   # ❌ not a dcat:Dataset → exactly 1 violation
