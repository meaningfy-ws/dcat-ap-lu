@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix ex:   <http://example.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .

###############################################################
# INVALID — guaranteed 1 violation (wrong class)
###############################################################

ex:notDataset a foaf:Person .     # ❌ NOT a dcat:Dataset

ex:dataset-invalid a dcat:Dataset ;
    dct:title "Invalid wrong class" ;
    dct:identifier "ds-invalid" ;
    dct:description "Minimal valid metadata" ;
    dcat:isVersionOf ex:notDataset .   # ❌ violates sh:class — 1 violation
